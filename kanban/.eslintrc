{
    "plugins": [
      "prettier"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended", 
      "prettier/@typescript-eslint", 
      "plugin:prettier/recommended" 
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": 0,
      "prettier/prettier": ["error", {"endOfLine": "auto"}]
    }
  }